[geometric_structure]
	xmlfile = geometries/tree_fem_membrane.xml
	xmldeformer = datafiles/SolverParamList.xml
	geometries_dir = ../../../meshes/
[../]

[assembler]
	type = navierstokes_membrane
	[./stabilization]
	    type = supg  # to choose in {none, supg, ip}
	    gamma_pressure = 0.2  # ip pressure coefficient
    [../]
[../]

[bc_conditions]
    inlet_bc_type = dirichlet
