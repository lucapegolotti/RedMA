	numoutletbcs = 2
	pimfile = datafiles/IM_pressure.txt
    [./outlet0]
        type = windkessel
        blockindex = 5
        boundaryflag = 2

        C = 0.0000760  # WK3
        Rp = 718.27  # WK3
        Rd = 15945.20  # WK3
    [../]

    [./outlet1]
        type = windkessel
        blockindex = 8
        boundaryflag = 2

        C = 0.0000760  # WK3
        Rp = 718.27  # WK3
        Rd = 15945.20  # WK3
    [../]

	ring_constraint = full  # type of homogeneous BC imposed at I/O rings
[../]

[rb]
	[./offline]
		[./snapshots]
			number 			     = 50
			bound 			     = 0.2
