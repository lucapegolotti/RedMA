#-------------------------------------------------------------
#           Data file for NavierStokesExample
#-------------------------------------------------------------

[geometric_structure]
	xmlfile = geometries/pipe1.xml
	xmldeformer = datafiles/SolverParamList.xml
	geometries_dir = ../../../meshes/
[../]

[assembler]
	type = stokes
[../]

[bc_conditions]
	lifting = true
[../]

[preconditioner]
	options    = datafiles/solversOptionsFast
	outer      = saddlepoint
	inner	   = SIMPLE
	approxshur = SIMPLE
	innertol   = 0.5
	recompute  = true
[../]

[newton_method]
	tol = 1e-7
	maxit = 10
[../]

[fluid]
	velocity_order = P2
	pressure_order = P1
	viscosity      = 0.035
	density        = 1
	inflow_bc      = dirichlet
[../]

[structure]
	time_integration_order = 3
	poisson      = 0.3
	num_young    = 1
	young    	 = 3.e+6 # 1.07e7 4.07e6'
	thickness    = 0.1 # 0.2 0.15'
	density	     = 1.2
	flag = 10
	[./external_wall]
		elastic = 1e4
		plastic = 1e4
	[../]
[../]

[time_discretization]
	algorithm = alpha1storderp
	rhoinf = 0.5
	firstorderpressure = true # this should be set to true only when comparing with simvascular
	order = 2
	t0 = 0
	T = 0.01
	dt = 0.01
	solverstatistics = solverstatistics.txt
[../]

[exporter]
	outdir	   = solutions/
	type       = vtk
	start      = 0
	save_every = 1
[../]

[coupling]
	type                = chebyshev
	nMax                = 2
	stab_coefficient	= 1e-15
	check_stabterm		= false
[../]
